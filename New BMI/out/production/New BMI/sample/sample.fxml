    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <VBox alignment="CENTER" prefHeight="331.0" prefWidth="339.0">
            <children>
                <TextField fx:id="WeightTextField" promptText="Enter your weight (kg)" />
                <TextField fx:id="HeightTextField" promptText="Enter your height (m)" />
                <Button fx:id="SubmitButton" alignment="CENTER" mnemonicParsing="false" text="Submit" textAlignment="CENTER" />
                <Label fx:id="CategoryLabel" />
            </children></VBox>
    </children>
